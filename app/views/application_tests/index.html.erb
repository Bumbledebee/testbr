<p id="notice"><%= notice %></p>

<h1>Application Tests</h1>

<table>
  <thead>
    <tr>
      <th>Application</th>
      <th>Test</th>
      <th>Test value</th>
      <th>Test status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @application_tests.each do |application_test| %>
      <tr>
        <td><%= application_test.application_id %></td>
        <td><%= application_test.test_id %></td>
        <td><%= application_test.test_value_id %></td>
        <td><%= application_test.test_status_id %></td>
        <td><%= link_to 'Show', application_test %></td>
        <td><%= link_to 'Edit', edit_application_test_path(application_test) %></td>
        <td><%= link_to 'Destroy', application_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application Test', new_application_test_path %>
